<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fatidecoraciones.interfaz.controllers.producto.ProductoController">
  <Button onAction="#nuevoProducto" prefWidth="103.0" text="Nuevo Producto">
      <VBox.margin>
         <Insets top="20.0" />
      </VBox.margin></Button>
  <TableView fx:id="productoTableView" prefWidth="562.0">
    <columns>
      <TableColumn fx:id="idColumn" text="Id" />
      <TableColumn fx:id="artColumn" text="Articulo" />
      <TableColumn fx:id="nombreColumn" text="Nombre" />
      <TableColumn fx:id="precioColumn" text="Precio" />
      <TableColumn fx:id="esTelaColumn" text="Es Tela?" />
      <TableColumn fx:id="marcaColumn" text="Es Tela?" />
      <TableColumn fx:id="accionesColumn" text="Acciones" />

    </columns>
  </TableView>
</VBox>
